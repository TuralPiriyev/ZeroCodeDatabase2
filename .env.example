# Frontend (do NOT commit real secrets)
REACT_APP_PAYPAL_CLIENT_ID=
REACT_APP_PAYPAL_PLAN_ID=
REACT_APP_PAYPAL_ENV=sandbox # or production

# Server (keep this in server .env, never expose to client)
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
PAYPAL_API_BASE=
# MysterAI credentials (replace with your deployment secrets)
MYSTER_API_KEY=sk_your_myster_key_here
MYSTER_API_BASE_URL=https://api.myster.example

# Backwards-compatible entries (optional)
HF_KEY=sk_your_key_here # Optional: legacy Hugging Face key; kept for backward compatibility if still used
OPENAI_API_KEY=sk_your_key_here # Optional: legacy OpenAI key; proxy will accept OPENAI_API_KEY if no MYSTER_API_KEY/HF_KEY is set
